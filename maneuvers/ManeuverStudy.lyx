\begin_inset Formula 
\begin{eqnarray*}
W_{\parallel} & = & W\thinspace(\cos\delta\cos\psi+\sin\delta\sin\psi)\\
W_{\perp} & = & W\thinspace(\cos\delta\cos(\psi+\frac{\pi}{2})+\sin\delta\sin(\psi+\frac{\pi}{2}))\\
 & = & W\thinspace(-\cos\delta\sin\psi+\sin\delta\cos\psi)
\end{eqnarray*}

\end_inset


