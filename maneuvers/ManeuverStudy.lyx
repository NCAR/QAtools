cfit <- function (D) {
\end_layout

\begin_layout Plain Layout

  mwd <- mean (D$WD, na.rm=TRUE)
\end_layout

\begin_layout Plain Layout

  D$xi <- (Cradeg * (D$THDG + D$SSLIP * cos (D$ROLL*Cradeg) - D$AKRD * sin
 (D$ROLL*Cradeg))+2*pi) %% (2*pi)
\end_layout

\begin_layout Plain Layout

  Dc <- D[abs(D$ROLL) > 26, ]
\end_layout

\begin_layout Plain Layout

  DR <- Dc[Dc$ROLL > 0, ]
\end_layout

\begin_layout Plain Layout

  DL <- Dc[Dc$ROLL < 0, ]
\end_layout

\begin_layout Plain Layout

  DL$xi <- (DL$xi - mean(DL$WD, na.rm=TRUE)*Cradeg + 2*pi) %% (2*pi)
\end_layout

\begin_layout Plain Layout

  DR$xi <- (DR$xi - mean(DR$WD, na.rm=TRUE)*Cradeg + 2*pi) %% (2*pi)
\end_layout

\begin_layout Plain Layout

  Dc$xi <- (Dc$xi - mean(Dc$WD, na.rm=TRUE)*Cradeg + 2*pi) %% (2*pi)
\end_layout

\begin_layout Plain Layout

  fmC <- lm (WS ~ I(cos(xi))+I(TASX*sin(xi)), data=Dc)
\end_layout

\begin_layout Plain Layout

  fmL <- lm (WS ~ I(cos(xi))+I(TASX*sin(xi)), data=DL)
\end_layout

\begin_layout Plain Layout

  fmR <- lm (WS ~ I(cos(xi))+I(TASX*sin(xi)), data=DR)
\end_layout

\begin_layout Plain Layout

  cfC <- coefficients (fmC)
\end_layout

\begin_layout Plain Layout

  cfL <- coefficients (fmL)
\end_layout

\begin_layout Plain Layout

  cfR <- coefficients (fmR) 
\end_layout

\begin_layout Plain Layout

  xi <- (0:360)*pi/180
\end_layout

\begin_layout Plain Layout

  EBL <- binStats (DL[, c("WS", "xi")], bins=30)
\end_layout

\begin_layout Plain Layout

  EBL$xc <- EBL$xc / Cradeg
\end_layout

\begin_layout Plain Layout

  EBR <- binStats (DR[, c("WS", "xi")], bins=30)
\end_layout

\begin_layout Plain Layout

  EBR$xc <- EBR$xc / Cradeg
\end_layout

\begin_layout Plain Layout

  EBC <- binStats (Dc[, c("WS", "xi")], bins=30)
\end_layout

\begin_layout Plain Layout

  EBC$xc <- EBC$xc / Cradeg 
\end_layout

\begin_layout Plain Layout

  return(list(cfL, cfR, cfC, EBL, EBR, EBC))
\end_layout

\begin_layout Plain Layout

}
\end_layout

