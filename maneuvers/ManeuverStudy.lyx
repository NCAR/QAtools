\begin_layout Plain Layout

csq <- function (x, .d) {
\end_layout

\begin_layout Plain Layout

  roll <- .d$ROLL * pi / 180
\end_layout

\begin_layout Plain Layout

  gamma <- (.d$THDG + x[4] + .d$SSLIP*cos(roll) - .d$ATTACK*sin(roll)) * pi
 / 180 
\end_layout

\begin_layout Plain Layout

  dvx <- (.d$TASX+x[3])*sin(gamma) -x[1] - .d$GGVEW
\end_layout

\begin_layout Plain Layout

  dvy <- (.d$TASX+x[3])*cos(gamma) -x[2] - .d$GGVNS
\end_layout

\begin_layout Plain Layout

  chisq <- sum (dvx**2 + dvy**2)
\end_layout

\begin_layout Plain Layout

}
\end_layout

